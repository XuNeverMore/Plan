<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="style" parent="Theme.AppCompat">

    </style>


    <style name="overFlowStyle" parent="Theme.AppCompat">
        <item name="overlapAnchor">false</item>
    </style>

    <style name="poStyle" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/scale_big</item>
        <item name="android:windowExitAnimation">@anim/scale_small</item>
    </style>



</resources>
